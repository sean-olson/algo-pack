{
  "name": "algo-pack",
  "version": "0.1.1",
  "description": "a library of common solved algorithms implemneted in JavaScript",
  "main": "lib/index.js",
  "scripts": {
    "dev": "tsc --watch",
    "build": "tsc",
    "test": "jest",
    "lint": "eslint **/*.ts",
    "prepare": "npm run build",
    "prepublishOnly": "npm test && npm run lint",
    "preversion": "npm run lint",
    "version": "npm run build && git add --all src",
    "postversion": "git push && git push --tags"
  },
  "files": [
    "lib/**/*"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/sean-olson/algo-pack.git"
  },
  "keywords": [
    "Algorithms"
  ],
  "author": "Sean Olson",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/sean-olson/algo-pack/issues"
  },
  "homepage": "https://github.com/sean-olson/algo-pack#readme",
  "devDependencies": {
    "@types/jest": "^26.0.15",
    "@typescript-eslint/eslint-plugin": "^4.8.2",
    "@typescript-eslint/parser": "^4.8.2",
    "eslint": "^7.14.0",
    "jest": "^26.6.3",
    "ts-jest": "^26.4.4",
    "ts-node": "^9.0.0",
    "typescript": "^4.1.2"
  }
}
